{
  "name": "server",
  "version": "1.0.0",
  "description": "SAntry server side",
  "main": "index.js",
  "scripts": {
    "dev": "nodemon",
    "start": "tsc && node build/index.js",
    "build": "webpack",
    "test": "jest --detectOpenHandles --forceExit"
  },
  "jest": {
    "transform": {
      "^.+\\.ts$": "ts-jest"
    },
    "testRegex": "\\.test\\.ts$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json"
    ],
    "testEnvironment": "node",
    "globals": {
      "ts-jest": {
        "diagnostics": true
      }
    },
    "moduleNameMapper": {
      "^@root/(.*)$": "<rootDir>/src/$1",
      "^@test/(.*)$": "<rootDir>/test/$1",
      "^@api/(.*)$": "<rootDir>/src/api/$1",
      "^@middlewares(.*)$": "<rootDir>/src/api/middlewares/$1",
      "^@middlewares/(.*)$": "<rootDir>/src/api/middlewares/$1",
      "^@config/(.*)$": "<rootDir>/src/config/$1",
      "^@interfaces/(.*)$": "<rootDir>/src/interfaces/$1",
      "^@loaders(.*)$": "<rootDir>/src/loaders/$1",
      "^@loaders/(.*)$": "<rootDir>/src/loaders/$1",
      "^@models(.*)$": "<rootDir>/src/models/$1",
      "^@models/(.*)$": "<rootDir>/src/models/$1",
      "^@utils(.*)$": "<rootDir>/src/utils/$1",
      "^@utils/(.*)$": "<rootDir>/src/utils/$1",
      "^@services/(.*)$": "<rootDir>/src/services/$1"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/boostcamp-2020/Project11-A-Web-FE-Performance-Monitoring-Server.git"
  },
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/boostcamp-2020/Project11-A-Web-FE-Performance-Monitoring-Server/issues"
  },
  "homepage": "https://github.com/boostcamp-2020/Project11-A-Web-FE-Performance-Monitoring-Server#readme",
  "dependencies": {
    "ajv": "^7.0.0-beta.7",
    "bcrypt": "^5.0.0",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "got": "^11.8.0",
    "http-errors": "^1.7.2",
    "jsonwebtoken": "^8.5.1",
    "moment": "^2.29.1",
    "mongodb": "^3.6.3",
    "mongoose": "^5.10.15",
    "mongoose-paginate-v2": "^1.3.11",
    "morgan": "^1.10.0",
    "nodemailer": "^6.4.16",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "swagger-jsdoc": "^5.0.1",
    "swagger-ui-express": "^4.1.5",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@types/bcrypt": "^3.0.0",
    "@types/compression": "^1.7.0",
    "@types/cookie-parser": "^1.4.2",
    "@types/cors": "^2.8.8",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.9",
    "@types/http-errors": "^1.8.0",
    "@types/jest": "^26.0.18",
    "@types/mongodb": "^3.5.34",
    "@types/mongoose": "^5.10.1",
    "@types/mongoose-paginate-v2": "^1.3.8",
    "@types/morgan": "^1.9.2",
